<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="time.com.timetable" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="time.com.timetable.MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="time.com.timetable.TimeTableModule"
            android:label="@string/title_activity_time_table_module" >
        </activity>
        <activity
            android:name="time.com.timetable.DataListActivity"
            android:label="@string/title_activity_data_list" >
        </activity>
    </application>

</manifest>
